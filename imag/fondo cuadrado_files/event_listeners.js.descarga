document.addEventListener("DOMContentLoaded",function(){let classNames=['lazyload','lazyloading','lazyloaded'];let lazyImages=[].concat([].slice.call(document.querySelectorAll("img.lazyload")),[].slice.call(document.querySelectorAll("img.lazyloaded")),[].slice.call(document.querySelectorAll("img.lazyloading")));let active=false;const lazyLoad=function(){if(active===false){active=true;setTimeout(function(){lazyImages.forEach(lazyImage=>{if((lazyImage.getBoundingClientRect().top<=window.innerHeight&&lazyImage.getBoundingClientRect().bottom>=0)&&getComputedStyle(lazyImage).display!=="none"){if(lazyImage.dataset.src===undefined||lazyImage.dataset.srcset===undefined){return;}
lazyImage.src=lazyImage.dataset.src;lazyImage.srcset=lazyImage.dataset.srcset;classNames.forEach(item=>{lazyImage.classList.remove(item);});lazyImages=lazyImages.filter(image=>{return image!==lazyImage;});if(lazyImages.length===0){document.removeEventListener("scroll",lazyLoad);window.removeEventListener("resize",lazyLoad);window.removeEventListener("orientationchange",lazyLoad);}}});active=false;},200);}};document.addEventListener("scroll",lazyLoad);window.addEventListener("resize",lazyLoad);window.addEventListener("orientationchange",lazyLoad);let images=[].slice.call(document.querySelectorAll("img"));const manageImgSizeTags=function(){setTimeout(function(){images.forEach(image=>{image.setAttribute('height',image.height);image.setAttribute('width',image.width);if(typeof image.alt==='undefined'||!image.alt){image.setAttribute('alt','Content image of PIXERS.PL');}});},500);};window.addEventListener("resize",manageImgSizeTags);window.addEventListener("orientationchange",manageImgSizeTags);});document.addEventListener('DOMContentLoaded',()=>{document.querySelectorAll('a').forEach(anchor=>{if(anchor.href.includes('?')){anchor.setAttribute('rel','nofollow');}});});